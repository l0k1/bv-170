<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <!-- put airspeed next to rpm
  
		x should be ~.144 (forward/back)
		y is left/right
		z is up/down
  -->
  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- AI -->
  <model>
    <path>Aircraft/BV-170/Models/Interior/Panel/Instruments/ai/ai.xml</path>
    <offsets>
      <x-m> 0.167 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.060 </z-m>
    </offsets>
  </model>

  <!-- ASI -->
  <model>
    <path>Aircraft/BV-170/Models/Interior/Panel/Instruments/airspeed/airspeed.xml</path>
    <offsets>
      <x-m>  0.167 </x-m>
      <y-m>  0.120 </y-m>
      <z-m>  0.045 </z-m>
    </offsets>
  </model>

